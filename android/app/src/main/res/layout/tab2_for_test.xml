<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="47dp"
        android:background="@drawable/upper_tab">

        <ImageButton
            android:id="@+id/tab2_searchlist_back_arrow"
            android:layout_width="10dp"
            android:layout_height="15.9dp"
            android:layout_marginTop="17dp"
            android:layout_marginLeft="14dp"
            android:background="@drawable/back_arrow"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#000000"
            android:textSize="16sp"
            android:lineSpacingExtra="18sp"
            android:fontFamily="@font/ns_extrabold"
            android:text="상세 검색 결과">
        </TextView>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity ="center"
            android:layout_weight="1"
            android:layout_marginTop="5dp"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/carwashlist_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/car_wash_review_box_no_pic"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/carwashlist_image"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_marginLeft="21dp"
                        android:layout_marginTop="15dp"
                        android:src="@drawable/box_image" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15.5dp"
                        android:layout_marginTop="25dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/carwashlist_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/ns_bold"
                            android:lineSpacingExtra="40sp"
                            android:text="CMC모터스 강남점"
                            android:textColor="@color/color_black"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <RatingBar
                                android:id="@+id/carwashlist_rating_bar"
                                style="@style/Widget.AppCompat.RatingBar.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:isIndicator="true"
                                android:rating="3.2"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:stepSize="0.5"
                                android:theme="@style/RatingBar"
                                android:transformPivotX="0dp"
                                android:transformPivotY="0dp" />


                            <TextView
                                android:id="@+id/carwashlist_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"
                                android:textSize="10sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/carwashlist_wash"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/ns_bold"
                            android:lineSpacingExtra="2sp"
                            android:text="손세차"
                            android:textColor="@color/color_black"
                            android:textSize="10sp" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/ns_bold"
                                android:lineSpacingExtra="2sp"
                                android:text="@string/car_wash_time"
                                android:textColor="#000000"
                                android:textSize="10sp" />

                            <TextView
                                android:id="@+id/carwashlist_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/ns_bold"
                                android:lineSpacingExtra="2sp"
                                android:text="09:00-18:00"
                                android:textColor="#000000"
                                android:textSize="10sp" />

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/carwashlist_layout2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:background="@drawable/car_wash_review_box_no_pic">

                    <ImageView
                        android:id="@+id/carwashlist_image2"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="21dp"
                        android:src="@drawable/box_image" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="15.5dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/carwashlist_name2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:text="MKCarWash"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="40sp"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <RatingBar
                                android:id="@+id/carwashlist_rating_bar2"
                                style="@style/Widget.AppCompat.RatingBar.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:isIndicator="true"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:transformPivotX="0dp"
                                android:transformPivotY="0dp"
                                android:rating="4.8"
                                android:stepSize="0.5"
                                android:theme="@style/RatingBar"/>


                            <TextView
                                android:id="@+id/carwashlist_score2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:textColor="#000000"
                                android:layout_gravity="center_vertical"
                                android:fontFamily="@font/ns_bold"/>
                        </LinearLayout>

                        <TextView
                            android:id="@+id/carwashlist_wash2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:textSize="10sp"
                            android:text="출장세차, 실내세차"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="2sp"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"
                                android:text="@string/car_wash_time"/>

                            <TextView
                                android:id="@+id/carwashlist_time2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:text="09:00-22:00"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"/>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/carwashlist_layout3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:background="@drawable/car_wash_review_box_no_pic">

                    <ImageView
                        android:id="@+id/carwashlist_image3"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="21dp"
                        android:src="@drawable/box_image" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="15.5dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/carwashlist_name3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:text="119카플라자"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="40sp"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <RatingBar
                                android:id="@+id/carwashlist_rating_bar3"
                                style="@style/Widget.AppCompat.RatingBar.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:isIndicator="true"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:transformPivotX="0dp"
                                android:transformPivotY="0dp"
                                android:rating="4.6"
                                android:stepSize="0.5"
                                android:theme="@style/RatingBar"/>


                            <TextView
                                android:id="@+id/carwashlist_score3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:textColor="#000000"
                                android:layout_gravity="center_vertical"
                                android:fontFamily="@font/ns_bold"/>
                        </LinearLayout>

                        <TextView
                            android:id="@+id/carwashlist_wash3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:textSize="10sp"
                            android:text="손세차, 실내세차"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="2sp"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"
                                android:text="@string/car_wash_time"/>

                            <TextView
                                android:id="@+id/carwashlist_time3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:text="09:00-18:00"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"/>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/carwashlist_layout4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:background="@drawable/car_wash_review_box_no_pic">

                    <ImageView
                        android:id="@+id/carwashlist_image4"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="21dp"
                        android:src="@drawable/box_image" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="15.5dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/carwashlist_name4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="40sp"
                            android:text="세계오토메틱 손세차장"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <RatingBar
                                android:id="@+id/carwashlist_rating_bar4"
                                style="@style/Widget.AppCompat.RatingBar.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:isIndicator="true"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:transformPivotX="0dp"
                                android:transformPivotY="0dp"
                                android:rating="4"
                                android:stepSize="0.5"
                                android:theme="@style/RatingBar"/>


                            <TextView
                                android:id="@+id/carwashlist_score4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:textColor="#000000"
                                android:layout_gravity="center_vertical"
                                android:fontFamily="@font/ns_bold"/>
                        </LinearLayout>

                        <TextView
                            android:id="@+id/carwashlist_wash4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="손세차, 실내세차"
                            android:textSize="10sp"
                            android:textColor="@color/color_black"
                            android:lineSpacingExtra="2sp"
                            android:fontFamily="@font/ns_bold"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="10sp"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"
                                android:text="@string/car_wash_time"/>

                            <TextView
                                android:id="@+id/carwashlist_time4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="09:00-18:00"
                                android:textSize="10sp"
                                android:lineSpacingExtra="2sp"
                                android:fontFamily="@font/ns_bold"
                                android:textColor="#000000"/>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </FrameLayout>



</LinearLayout>